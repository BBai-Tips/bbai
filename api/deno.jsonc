{
	"name": "bbai-api",
	"version": "0.0.1",
	"exports": "./src/main.ts",
	"tasks": {
		"start": "deno run --allow-net --allow-env src/main.ts",
		"dev": "deno run --watch --allow-net --allow-env src/main.ts",
		"test": "deno test --allow-net --allow-env",
		"format": "deno fmt",
		"check-types": "deno check src/main.ts",
		"update-deps": "deno cache src/main.ts && deno cache tests/deps.ts"
	},
	"imports": {
		"anthropic": "https://esm.sh/@anthropic-ai/sdk@0.22.0",
		"anthropic-tokenizer": "npm:@anthropic-ai/tokenizer",
		"openai": "https://esm.sh/openai@4.24.0",
		"groq": "npm:groq-sdk@0.5.0",
		"ajv": "https://esm.sh/ajv@8.14.0",
		"swagger-jsdoc": "npm:swagger-jsdoc@6.2.8",
		"common-tags": "https://esm.sh/common-tags@1.8.2",
		"md5": "https://esm.sh/md5@2.3.0",
		"json-schema": "https://esm.sh/@types/json-schema@7.0.15/index.d.ts",
		"@oak/commons": "jsr:@oak/commons@^0.11.0",
		"@oak/oak": "jsr:@oak/oak@^16.0.0",
		"oak_middleware": "https://deno.land/x/oak_middleware/mod.ts",
		"oak_logger": "https://deno.land/x/oak_logger@1.0.0/mod.ts",
		"cors": "https://deno.land/x/cors@v1.2.2/mod.ts",
		"ms": "https://deno.land/x/ms@v0.1.0/ms.ts",
		"@std/datetime": "jsr:@std/datetime@^0.224.0",
		"@std/dotenv": "jsr:@std/dotenv@^0.224.0",
		"@std/fs": "jsr:@std/fs@^0.224.0",
		"@std/path": "jsr:@std/path@^0.225.1",
		"@std/io": "jsr:@std/io@^0.224.1",
		"@std/log": "jsr:@std/log@^0.224.1",
		"@std/ulid": "jsr:@std/ulid@^0.224.1",
		"@std/uuid": "jsr:@std/uuid@^0.224.3",
		"@std/crypto": "jsr:@std/crypto@^0.224.0",
		"@std/encoding/hex": "jsr:@std/encoding@^0.224.3/hex",
		"@std/testing/mock": "jsr:@std/testing@^0.224.0/mock",
		"@std/assert": "jsr:@std/assert@^0.225.3",
		"shared/types.ts": "../src/shared/types.ts"
		"shared/logger.ts": "../src/shared/logger.utils.ts"
	},
	"unstable": ["kv"],
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"semiColons": true,
		"singleQuote": true,
		"proseWrap": "preserve",
		"include": ["src/", "tests/"],
		"exclude": ["src/testdata/", "src/fixtures/**/*.ts"]
	},
	"lint": {
		"files": {
			"include": ["evals/xestroAiPipelineProvider.js"]
		},
		"rules": {
			"exclude": ["require-await", "no-var"]
		}
	}
}
